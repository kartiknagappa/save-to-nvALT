{
  "manifest_version": 2,
  "name": "Send to nvALT",
  "version": "1.0",
  "description": "Send pages to nvALT via toolbar or context menu. Requires OS X and nvALT 2.1+.",
  "permissions": [
    "contextMenus",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "nvIt"
  },
  "icons": {
    "16":"icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["global.js"]
  },
  "minimum_chrome_version": "14.0"
}
